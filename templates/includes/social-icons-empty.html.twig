{%
  set social_classes = [
    'social-icons'
    cellular.display_social_round_icons ? 'round',
  ];
  set obj = cellular.social_profiles; 
  set social_follow = [
    social_facebook,
    social_instagram,
    social_linkedin,
    social_youtube,
    social_google,
    social_twitter,
    social_github
  ];
  set social_share = [
    social_facebook,
    social_google,
    social_linkedin,
    social_twitter,
    social_reddit
  ];
%}

<ul {{social_attribute.addClass(social_classes) }} >
 
 <li>
    <a href="#" class="facebook" target="_blank">
      <i class="fab fa-facebook"></i>
    </a>
  </li>
  
</ul>

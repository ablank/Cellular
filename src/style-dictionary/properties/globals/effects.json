{
    "animation":{
        "speed": {
          "value": "{speed.default.value}"
        }
    },
    "scale": {
      "start": {
        "value": "0"
      },
      "end": {
        "value": "1"
      }
    },
    "shadow": {
      "default": {
        "value": "0 0 {size.small.value} {size.small.value}"
      },
      "small": {
        "value": "0 0 {size.tiny.value} {size.tiny.value}"
      },
      "large": {
        "value": "0 0 {size.default.value} {size.default.value}"
      }
    }
  }
  
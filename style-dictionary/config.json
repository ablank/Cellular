{
  "source": [
    "./style-dictionary/properties/**/*.json"
  ],
  "platforms": {
    "scss": {
      "transformGroup": "scss",
      "buildPath": "src/",
      "files": [{
        "format": "scss/variables",
        "destination": "sass/_style-dictionary.scss"
      }]
    },
    "js": {
      "transformGroup": "js",
      "buildPath": "src/",
      "files": [{
        "format": "json",
        "destination": "js/style-dictionary.json"
      }]
    }
  }
}